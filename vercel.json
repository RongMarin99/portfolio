{
   "version": 2,
   "builds": [
     {
       "src": "package.json",
       "use": "@vercel/node"
     },
     {
       "src": "nuxt.config.js",
       "use": "@nuxtjs/vercel-builder",
       "config": {
         "serverFiles": [
           "static/**",
           ".nuxt/dist/sitemap-routes.json"
         ]
       }
     }
   ],
   "buildCommand": "yarn build",
   "outputDirectory": "dist",
   "framework": "nuxtjs"
 }
 